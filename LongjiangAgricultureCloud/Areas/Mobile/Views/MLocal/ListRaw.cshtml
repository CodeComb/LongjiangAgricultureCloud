@model IEnumerable<Information>
@{
    Layout = null;
}
@foreach (var i in Model)
{
    <a href="/Mobile/MLocal/Show/@i.ID">
        <div class="list-outer">
            @i.Title<span class="recommand-time">@i.Time.ToString("MM-dd")</span><br /><div class="sub-title">类型：@i.SupplyDemand / 价格：@i.Price</div>
        </div>
    </a>
}